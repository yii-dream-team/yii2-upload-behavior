{
  "name": "bajadev/yii2-upload-behavior",
  "type": "yii2-extension",
  "description": "Yii2 file/image upload behavior for ActiveRecord",
  "keywords": [
    "file",
    "image",
    "upload",
    "behavior",
    "resize",
    "thumb"
  ],
  "license": "MIT",
  "homepage": "http://bajadev.hu",
  "authors": [
    {
      "name": "Bajadev",
      "email": "info@bajadev.hu",
      "homepage": "http://bajadev.hu",
      "role": "Developer"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-imagine": "2.3"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Bajadev/yii2-upload-behavior"
    },
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "bajadev\\upload\\": "./src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
